{
    "name":  "serious-sez",
    "version":  "2.0.0",
    "scripts":  {
                    "ng":  "ng",
                    "start":  "ng serve",
                    "build":  "ng build --watch --deploy-url /serioussez/ --base-href /serioussez/ --configuration=development",
                    "build-prod":  "ng build --configuration production",
                    "watch":  "ng build --watch --configuration development",
                    "test":  "ng test",
                    "iv":  "powershell -ExecutionPolicy Bypass -File ./increment-version.ps1",
                    "iv:minor":  "powershell -ExecutionPolicy Bypass -File ./increment-version.ps1 -BumpType minor",
                    "iv:major":  "powershell -ExecutionPolicy Bypass -File ./increment-version.ps1 -BumpType major",
                    "iv:feature":  "powershell -ExecutionPolicy Bypass -File ./increment-version.ps1 -BumpType feature"
                },
    "private":  true,
    "dependencies":  {
                         "@angular/animations":  "^21.1.0",
                         "@angular/common":  "^21.1.0",
                         "@angular/compiler":  "^21.1.0",
                         "@angular/core":  "^21.1.0",
                         "@angular/forms":  "^21.1.0",
                         "@angular/platform-browser":  "^21.1.0",
                         "@angular/platform-browser-dynamic":  "^21.1.0",
                         "@angular/router":  "^21.1.0",
                         "@animxyz/core":  "^0.6.4",
                         "@auth0/angular-jwt":  "^5.2.0",
                         "@fortawesome/free-solid-svg-icons":  "^5.15.3",
                         "@kolkov/angular-editor":  "2.1.0",
                         "@rxjs/rx":  "^4.1.0",
                         "bootstrap":  "^5.0.2",
                         "ckeditor4-angular":  "^5.2.1",
                         "ngx-clipboard":  "^14.0.1",
                         "ngx-image-cropper":  "^9.1.6",
                         "rxjs":  "^7.8.1",
                         "tslib":  "^2.6.2",
                         "uuid":  "^8.3.2",
                         "zone.js":  "~0.15.1"
                     },
    "devDependencies":  {
                            "@angular/build":  "^21.1.0",
                            "@angular/cli":  "^21.1.0",
                            "@angular/compiler-cli":  "^21.1.0",
                            "@types/jasmine":  "~3.6.0",
                            "@types/node":  "^12.11.1",
                            "@types/uuid":  "^8.3.1",
                            "jasmine-core":  "~3.7.0",
                            "karma":  "~6.4.4",
                            "karma-chrome-launcher":  "~3.1.0",
